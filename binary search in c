#include <stdio.h>
int binarySearch(int array[], int x, int low, int high) 
{
  while (low <= high) 
  {
    int mid =  (high + low) / 2;

    if (array[mid] == x)
      return mid;

    if (array[mid] < x)
      low = mid + 1;

    else
      high = mid - 1;
  }

  return -1;
}

int main(void) {
  int array[50],n,x;
  printf("enter number of Elements: ");
  scanf("%d",&n);
  printf("enter target: ");
  scanf("%d",&x);
  for(int i=0;i<n;i++)
  {
      scanf("%d",&array[i]);
  }
  int result = binarySearch(array, x, 0, n - 1);
  if (result == -1)
    printf("Not found");
  else
    printf("Element is found at index %d", result);
  return 0;
}
